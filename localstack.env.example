# LocalStack Configuration
# Copie este arquivo para localstack.env e ajuste conforme necessário
# Uso: source localstack.env

export AWS_ACCESS_KEY_ID=test
export AWS_SECRET_ACCESS_KEY=test
export AWS_DEFAULT_REGION=us-west-2
export AWS_ENDPOINT_URL=http://localhost:4566
export USE_LOCALSTACK=true

# Database Configuration (for local development)
export DATABASE_URL=postgresql://root:password@localhost:5432/webapp

echo "✅ Variáveis de ambiente LocalStack configuradas!"
echo "   AWS_ENDPOINT_URL: $AWS_ENDPOINT_URL"
echo "   AWS_DEFAULT_REGION: $AWS_DEFAULT_REGION"
